<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.capstone">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:usesCleartextTraffic="true"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".group.AddNewGroup"/>
        <activity android:name=".group.InputInvitation"/>
        <activity android:name=".group.SuccessEnterInvitation"/>
        <activity android:name=".group.SuccessCreateGroup"/>
        <activity android:name=".MainBaseActivity"/>
        <activity android:name=".user.SignUp"/>
        <activity android:name=".user.SignIn"/>
        <activity android:name=".my.DetailActivity"/>
        <activity android:name=".my.NewBase"/>
        <activity android:name=".group.GroupMainCalendar"/>
        <activity android:name=".group.GroupApplierList"/>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>